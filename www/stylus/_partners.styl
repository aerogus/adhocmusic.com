/**
 * La liste des partenaires
 */

.partners
  display flex
  flex-wrap wrap

  li
    width 25%
    padding 1rem

    @media all and (max-width: 768px)
      &
        width 50%

    @media all and (max-width: 480px)
      &
        width 100%

    img
      float left
      padding-right 5px
      padding-bottom 5px
      width 50px
      height 50px
